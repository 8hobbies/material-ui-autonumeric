{
  "name": "mui-autonumeric",
  "description": "AutoNumeric-Powered MUI Components. AutoNumeric is a powerful library that automatically format numbers and currencies. MUI-AutoNumeric brings that power to MUI.",
  "version": "1.0.0",
  "type": "module",
  "browser": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist/*.d.ts",
    "/dist/*.js"
  ],
  "scripts": {
    "build_demo": "npm run compile && vite build",
    "clean": "rimraf dist/",
    "dev": "vite",
    "compile": "tsc --build . src/lib src/test src/demo",
    "coverage": "vitest run --coverage --typecheck",
    "doc": "typedoc && replace-in-files --string '=\"assets/' --replacement '=\"/assets/' docs/404.html && npm run build_demo",
    "format": "eslint --fix . && prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 && prettier . --check",
    "prepack": "npm run clean && npm run compile",
    "preview": "vite preview",
    "test": "vitest --typecheck",
    "test-once": "npm run coverage",
    "test-all": "npm run lint && npm run compile && npm run test-once && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/8hobbies/mui-autonumeric.git"
  },
  "author": "8 Hobbies, LLC",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/8hobbies/mui-autonumeric/issues"
  },
  "homepage": "https://mui-autonumeric.8hob.io",
  "keywords": [
    "react",
    "mui",
    "autonumeric"
  ],
  "dependencies": {
    "react-autonumeric": "^2.0.2"
  },
  "devDependencies": {
    "@8hobbies/eslint-conf-baseline": "^2.1.0",
    "@8hobbies/typedoc-plugin-404": "^2.2.0",
    "@8hobbies/typedoc-plugin-plausible": "^1.0.2",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.17",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "bootstrap": "^5.3.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.2.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "replace-in-files-cli": "^2.2.0",
    "rimraf": "^5.0.7",
    "typedoc": "^0.25.13",
    "typedoc-plugin-extra-footer": "^1.0.1",
    "typedoc-plugin-mdn-links": "^3.1.25",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@mui/material": "^5.15.17"
  },
  "peerDependenciesMeta": {
    "@mui/material": {
      "optional": true
    }
  }
}
